{
  "name": "mars2d-vue-example",
  "version": "3.2.0",
  "description": "在Vue3技术栈下的Mars2D平台 功能示例 演示项目",
  "scripts": {
    "serve": "vite --host",
    "dev": "vite",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "build2": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "serve:dist": "http-server ./dist",
    "lint": "vue-tsc --noEmit --skipLibCheck && npm run eslint",
    "eslint": "eslint ./src/**/*.{js,vue,ts} --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@icon-park/svg": "^1.4.2",
    "@marsgis/editor": "^1.2.0",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.7",
    "core-js": "^3.36.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "mars2d": "^3.2.0",
    "monaco-editor": "^0.44.0",
    "nprogress": "^0.2.0",
    "serve": "^14.2.1",
    "split.js": "^1.6.5",
    "standard": "^17.1.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-color-kit": "^1.0.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "consola": "^3.2.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.21.1",
    "fs-extra": "^11.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-copy": "~3.5.0",
    "serve-static": "^1.15.0",
    "standard": "^17.1.0",
    "terser": "^5.27.2",
    "typescript": "~5.2.2",
    "vite": "^4.5.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars2d-vue-example.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars2d-vue-example/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars2d",
    "leaflet",
    "vue",
    "vue3",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars2d.cn",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run eslint"
    ]
  }
}
